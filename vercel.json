{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "routes": [
        {
            "src": "resources/css/(.*)",
            "dest": "../resources/css/(.*)"
        },
        {
            "src": "resources/js/(.*)",
            "dest": "../resources/js/(.*)"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {  

    },
    "outputDirectory": "public"
}