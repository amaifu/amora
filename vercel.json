{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/resources/css/app.css"
        },
        {
            "src": "/(.*)",
            "dest": "/resources/js/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/resources/js/bootstrap.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {  

    },
    "outputDirectory": "public"
}